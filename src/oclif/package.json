{
  "name": "oclif-placeholder",
  "version": "0.0.1",
  "description": "A template for oclif project.",
  "author": "Moch. Nafkhan Alzamzami @nafkhanzam",
  "bin": {
    "oex": "./bin/run.js"
  },
  "homepage": "https://github.com/nafkhanzam/oclif-placeholder",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "nafkhanzam/oclif-placeholder",
  "bugs": "https://github.com/nafkhanzam/oclif-placeholder/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "type": "module",
  "oclif": {
    "bin": "oex",
    "dirname": "oex",
    "commands": {
      "strategy": "pattern",
      "target": "./dist/commands",
      "globPatterns": [
        "**/index.+(js|cjs|mjs|ts|tsx|mts|cts)",
        "!**/*.+(d.*|test.*|spec.*|helpers.*)?(x)"
      ]
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": ":"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md",
    "test": "jest"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@nafkhanzam/deno-utils": "npm:@jsr/nafkhanzam__deno-utils@0.1.1",
    "@oclif/core": "4.0.34",
    "@oclif/plugin-help": "6.2.18",
    "axios": "1.7.9",
    "fast-xml-parser": "4.5.0",
    "fs-extra": "11.2.0",
    "js-toml": "1.0.1",
    "yaml": "2.6.1",
    "zod": "3.23.8"
  },
  "resolutions": {
    "whatwg-url": "14.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.4",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.1",
    "@typescript-eslint/eslint-plugin": "8.17.0",
    "@typescript-eslint/parser": "8.17.0",
    "eslint": "9.16.0",
    "eslint-config-oclif": "5.2.2",
    "eslint-config-oclif-typescript": "3.1.12",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "15.13.0",
    "globby": "14.0.2",
    "jest": "29.7.0",
    "nyc": "17.1.0",
    "oclif": "4.16.0",
    "prettier": "3.4.2",
    "shx": "0.3.4",
    "ts-jest": "29.2.5",
    "tsconfig-paths-jest": "0.0.1",
    "tslib": "2.8.1",
    "tsx": "4.19.2",
    "typescript": "5.7.2"
  }
}
